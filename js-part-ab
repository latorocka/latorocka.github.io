@Test
public void testLogin() {
    LoginPage loginPage = new LoginPage(driver);
    loginPage.enterUsername("testuser");
    loginPage.enterPassword("password");
    loginPage.clickLoginButton();
    
    Assert.assertTrue(loginPage.isLoginSuccessful());
